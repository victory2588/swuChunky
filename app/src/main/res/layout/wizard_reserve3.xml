<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/reservation3_uploadbtn"
        android:layout_width="126dp"
        android:layout_height="45dp"
        android:layout_marginBottom="46dp"
        android:backgroundTint="#00b8ff"
        android:text="일정 등록하기"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/reservation3_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="72dp"
        android:text="청소 카테고리 설정"
        android:textColor="#0B0B0B"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/reservation3_txtexp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="청소를 원하는 항목을 선택해주세요"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/reservation3_txt" />

    <TextView
        android:id="@+id/living_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:gravity="center_horizontal"
        android:text="생활청소"
        android:textSize="12sp"
        app:layout_constraintTop_toBottomOf="@+id/reservation3_living"
        tools:layout_editor_absoluteX="57dp" />

    <TextView
        android:id="@+id/sofa_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:gravity="center_horizontal"
        android:text="가구.가전청소"
        android:textSize="12sp"
        app:layout_constraintTop_toBottomOf="@+id/reservation3_sofa"
        tools:layout_editor_absoluteX="210dp" />

    <TextView
        android:id="@+id/special_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:gravity="center_horizontal"
        android:text="특수청소"
        android:textSize="12sp"
        app:layout_constraintTop_toBottomOf="@+id/reservation3_special"
        tools:layout_editor_absoluteX="57dp" />

    <TextView
        android:id="@+id/animal_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:gravity="center_horizontal"
        android:text="반려동물"
        android:textSize="12sp"
        app:layout_constraintTop_toBottomOf="@+id/reservation3_animal"
        tools:layout_editor_absoluteX="305dp" />

    <TextView
        android:id="@+id/move_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:gravity="center_horizontal"
        android:text="이사청소"
        android:textSize="12sp"
        app:layout_constraintTop_toBottomOf="@+id/reservation3_move"
        tools:layout_editor_absoluteX="141dp" />

    <TextView
        android:id="@+id/trash_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:gravity="center_horizontal"
        android:text="분리수거"
        android:textSize="12sp"
        app:layout_constraintTop_toBottomOf="@+id/reservation3_trash"
        tools:layout_editor_absoluteX="224dp" />

    <TextView
        android:id="@+id/company_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:gravity="center_horizontal"
        android:text="사무실청소"
        android:textSize="12sp"
        app:layout_constraintTop_toBottomOf="@+id/reservation3_company"
        tools:layout_editor_absoluteX="141dp" />

    <TextView
        android:id="@+id/toilet_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:gravity="center_horizontal"
        android:text="화장실"
        android:textSize="12sp"
        app:layout_constraintTop_toBottomOf="@+id/reservation3_toilet"
        tools:layout_editor_absoluteX="316dp" />

    <CheckBox
        android:id="@+id/checkBox1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="에어컨"
        android:textSize="12sp"
        tools:layout_editor_absoluteX="45dp"
        tools:layout_editor_absoluteY="444dp" />

    <CheckBox
        android:id="@+id/checkBox6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="소파"
        android:textSize="12sp"
        tools:layout_editor_absoluteX="142dp"
        tools:layout_editor_absoluteY="444dp" />

    <CheckBox
        android:id="@+id/checkBox5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="냉장고"
        android:textSize="12sp"
        tools:layout_editor_absoluteX="142dp"
        tools:layout_editor_absoluteY="507dp" />

    <CheckBox
        android:id="@+id/checkBox3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="공기청정기"
        android:textSize="12sp"
        tools:layout_editor_absoluteX="237dp"
        tools:layout_editor_absoluteY="444dp" />

    <CheckBox
        android:id="@+id/checkBox6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="김치냉장고"
        android:textSize="12sp"
        tools:layout_editor_absoluteX="237dp"
        tools:layout_editor_absoluteY="507dp" />

    <CheckBox
        android:id="@+id/checkBox4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="선풍기"
        android:textSize="12sp"
        tools:layout_editor_absoluteX="44dp"
        tools:layout_editor_absoluteY="507dp" />

    <TextView
        android:id="@+id/reservation3_txtexp2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="*최소 3개 이상 선택"
        android:textColor="#ff8da4"
        android:textSize="10sp"
        tools:layout_editor_absoluteX="45dp"
        tools:layout_editor_absoluteY="175dp" />

    <ImageView
        android:id="@+id/reservation3_living"
        android:layout_width="72dp"
        android:layout_height="66dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="10dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/reservation3_txtexp2"
        app:srcCompat="@drawable/living" />

    <ImageView
        android:id="@+id/reservation3_move"
        android:layout_width="72dp"
        android:layout_height="66dp"
        android:layout_marginStart="11dp"
        app:layout_constraintStart_toEndOf="@+id/reservation3_living"
        app:layout_constraintTop_toTopOf="@+id/reservation3_living"
        app:srcCompat="@drawable/move" />

    <ImageView
        android:id="@+id/reservation3_sofa"
        android:layout_width="72dp"
        android:layout_height="66dp"
        android:layout_marginStart="11dp"
        android:layout_marginTop="9dp"
        app:layout_constraintStart_toEndOf="@+id/reservation3_move"
        app:layout_constraintTop_toBottomOf="@+id/reservation3_txtexp2"
        app:srcCompat="@drawable/sofa" />

    <ImageView
        android:id="@+id/reservation3_animal"
        android:layout_width="72dp"
        android:layout_height="66dp"
        android:layout_marginStart="11dp"
        android:layout_marginTop="9dp"
        android:layout_marginEnd="46dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/reservation3_sofa"
        app:layout_constraintTop_toBottomOf="@+id/reservation3_txtexp2"
        app:srcCompat="@drawable/animal" />

    <ImageView
        android:id="@+id/reservation3_special"
        android:layout_width="72dp"
        android:layout_height="66dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="36dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/living_txt"
        app:srcCompat="@drawable/special" />

    <ImageView
        android:id="@+id/reservation3_company"
        android:layout_width="72dp"
        android:layout_height="66dp"
        android:layout_marginStart="11dp"
        app:layout_constraintStart_toEndOf="@+id/reservation3_special"
        app:srcCompat="@drawable/company"
        tools:layout_editor_absoluteY="325dp" />

    <ImageView
        android:id="@+id/reservation3_trash"
        android:layout_width="72dp"
        android:layout_height="66dp"
        android:layout_marginStart="11dp"
        app:layout_constraintStart_toEndOf="@+id/reservation3_company"
        app:srcCompat="@drawable/trash"
        tools:layout_editor_absoluteY="325dp" />

    <ImageView
        android:id="@+id/reservation3_toilet"
        android:layout_width="72dp"
        android:layout_height="66dp"
        android:layout_marginStart="11dp"
        app:layout_constraintStart_toEndOf="@+id/reservation3_trash"
        app:srcCompat="@drawable/toilet"
        tools:layout_editor_absoluteY="325dp" />

</androidx.constraintlayout.widget.ConstraintLayout>