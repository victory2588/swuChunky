<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/reservation3_uploadbtn"
        android:layout_width="126dp"
        android:layout_height="45dp"
        android:layout_marginBottom="76dp"
        android:backgroundTint="#00b8ff"
        android:text="일정 등록하기"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/reservation3_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="72dp"
        android:text="청소 카테고리 설정"
        android:textColor="#0B0B0B"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/reservation3_txtexp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="청소를 원하는 항목을 선택해주세요"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/reservation3_txt" />

    <TextView
        android:id="@+id/living_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="57dp"
        android:layout_marginTop="8dp"
        android:gravity="center_horizontal"
        android:text="생활청소"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/reservation3_living" />

    <TextView
        android:id="@+id/sofa_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        android:layout_marginTop="8dp"
        android:gravity="center_horizontal"
        android:text="가구.가전청소"
        android:textSize="12sp"
        app:layout_constraintStart_toEndOf="@+id/move_txt"
        app:layout_constraintTop_toBottomOf="@+id/reservation3_sofa" />

    <TextView
        android:id="@+id/special_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="57dp"
        android:layout_marginTop="8dp"
        android:gravity="center_horizontal"
        android:text="특수청소"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/reservation3_special" />

    <TextView
        android:id="@+id/animal_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="63dp"
        android:gravity="center_horizontal"
        android:text="반려동물"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/sofa_txt"
        app:layout_constraintTop_toBottomOf="@+id/reservation3_animal" />

    <TextView
        android:id="@+id/move_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="8dp"
        android:gravity="center_horizontal"
        android:text="이사청소"
        android:textSize="12sp"
        app:layout_constraintStart_toEndOf="@+id/living_txt"
        app:layout_constraintTop_toBottomOf="@+id/reservation3_move" />

    <TextView
        android:id="@+id/trash_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:layout_marginTop="8dp"
        android:gravity="center_horizontal"
        android:text="분리수거"
        android:textSize="12sp"
        app:layout_constraintStart_toEndOf="@+id/company_txt"
        app:layout_constraintTop_toBottomOf="@+id/reservation3_trash" />

    <TextView
        android:id="@+id/company_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="8dp"
        android:gravity="center_horizontal"
        android:text="사무실청소"
        android:textSize="12sp"
        app:layout_constraintStart_toEndOf="@+id/special_txt"
        app:layout_constraintTop_toBottomOf="@+id/reservation3_company" />

    <TextView
        android:id="@+id/toilet_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="47dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="62dp"
        android:gravity="center_horizontal"
        android:text="화장실"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/trash_txt"
        app:layout_constraintTop_toBottomOf="@+id/reservation3_toilet" />

    <TextView
        android:id="@+id/reservation3_txtexp2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="45dp"
        android:layout_marginTop="31dp"
        android:text="*최소 3개 이상 선택"
        android:textColor="#ff8da4"
        android:textSize="10sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/reservation3_txtexp" />

    <ImageView
        android:id="@+id/reservation3_living"
        android:layout_width="72dp"
        android:layout_height="66dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="10dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/reservation3_txtexp2"
        app:srcCompat="@drawable/living" />

    <ImageView
        android:id="@+id/reservation3_move"
        android:layout_width="72dp"
        android:layout_height="66dp"
        android:layout_marginStart="11dp"
        app:layout_constraintStart_toEndOf="@+id/reservation3_living"
        app:layout_constraintTop_toTopOf="@+id/reservation3_living"
        app:srcCompat="@drawable/move" />

    <ImageView
        android:id="@+id/reservation3_sofa"
        android:layout_width="72dp"
        android:layout_height="66dp"
        android:layout_marginStart="11dp"
        android:layout_marginTop="9dp"
        app:layout_constraintStart_toEndOf="@+id/reservation3_move"
        app:layout_constraintTop_toBottomOf="@+id/reservation3_txtexp2"
        app:srcCompat="@drawable/sofa" />

    <ImageView
        android:id="@+id/reservation3_animal"
        android:layout_width="72dp"
        android:layout_height="66dp"
        android:layout_marginStart="11dp"
        android:layout_marginTop="9dp"
        android:layout_marginEnd="46dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/reservation3_sofa"
        app:layout_constraintTop_toBottomOf="@+id/reservation3_txtexp2"
        app:srcCompat="@drawable/animal" />

    <ImageView
        android:id="@+id/reservation3_special"
        android:layout_width="72dp"
        android:layout_height="66dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="36dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/living_txt"
        app:srcCompat="@drawable/special" />

    <ImageView
        android:id="@+id/reservation3_company"
        android:layout_width="72dp"
        android:layout_height="66dp"
        android:layout_marginStart="11dp"
        android:layout_marginTop="37dp"
        app:layout_constraintStart_toEndOf="@+id/reservation3_special"
        app:layout_constraintTop_toBottomOf="@+id/move_txt"
        app:srcCompat="@drawable/company" />

    <ImageView
        android:id="@+id/reservation3_trash"
        android:layout_width="72dp"
        android:layout_height="66dp"
        android:layout_marginStart="11dp"
        android:layout_marginTop="37dp"
        app:layout_constraintStart_toEndOf="@+id/reservation3_company"
        app:layout_constraintTop_toBottomOf="@+id/sofa_txt"
        app:srcCompat="@drawable/trash" />

    <ImageView
        android:id="@+id/reservation3_toilet"
        android:layout_width="72dp"
        android:layout_height="66dp"
        android:layout_marginStart="11dp"
        android:layout_marginTop="37dp"
        android:layout_marginEnd="46dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/reservation3_trash"
        app:layout_constraintTop_toBottomOf="@+id/animal_txt"
        app:srcCompat="@drawable/toilet" />

</androidx.constraintlayout.widget.ConstraintLayout>